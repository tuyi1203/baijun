<root>
 <table>
 <tbody>
{{foreach from=$list item="kv"}}
   <tr data-openid="{{$kv.openid}}" data-mid="{{$kv.id}}">
   <td class='text-center'>
           <a href='{:U('weixin/message/chat/default.html?openid={{$kv.openid}}')}' ><img class="image-small" width="64" height="64" src="{{$kv.headimgurl}}"></a></td>
   <td class='text-left'>{{$kv.name}}</td>
   <td class='text-left'>{{if isset($kv.content)}}{{$kv.content}}{{/if}}</td>
   <td class='text-center'>{:date("Y-m-d H:i:s" , {{$kv.createtime}})}</td>
   <!--
   <td class='text-center'>
     <a href='javascript:void(0);' class="quickreply">{{$la.message.quickreply}}</a>
   </td>
    -->
   </tr>
{{/foreach}}
</tbody>
</table>
<pagelink>
{{$pagelink}}
</pagelink>
</root>