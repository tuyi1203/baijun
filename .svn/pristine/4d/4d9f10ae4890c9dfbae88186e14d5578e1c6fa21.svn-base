{{include file="$WWW_PATH/header.html"}}
<body>
	{{include file="$WWW_PATH/datepicker.html"}}
    {{include file="$WWW_PATH/top_menu.html"}}

	<div class="clearfix">
    {{include file="$WWW_PATH/left_menu.html"}}
		<div class='col-md-10'>
			<div class='panel'>
				<div class='panel-heading'>
					<strong><i class='icon-plus'></i>&nbsp;{{$la.todaywater.create}}</strong>
				</div>
				<div class='panel-body'>
				<!-- <form id='sortForm'
					action='/chanzhieps/www/admin.php?m=slide&f=sort' method='post'>-->
				<form id='ajaxForm'
                    action='{:U('information/todaywater/add/insert.json')}' method='post'>
					<table class='table table-form'>
					   <tr>
				          <th class='w-100px'>{{$la.todaywater.day}}</th>
				          <td>
				          <div class="required required-wrapper"></div>
				          <input type='text' name='data[day]' id='day' value='{{$day}}' class='form-control date' style="width:180px;"/>
				          </td>
				          <td colspan="4"></td>
				       </tr>
				       <tr>
                          <th>{{$la.todaywater.gongshui}}1</th>
                          <td>
                          <div class="required required-wrapper"></div>
                          <input type='text' name='data[place1]' id='place1' value='' class="form-control" />
                          </td>
                          <th>{{$la.todaywater.arg1}}</th>
                          <td>
                          <div class="required required-wrapper"></div>
                          <input type='text' name='data[arg11]' id='arg11' value='' class="form-control" />
                          </td>
                          <th>{{$la.todaywater.arg2}}</th>
                          <td>
                          <div class="required required-wrapper"></div>
                          <input type='text' name='data[arg12]' id='arg12' value='' class="form-control" />
                          </td>
                       </tr>
                       <tr>
                          <th>{{$la.todaywater.gongshui}}2</th>
                          <td>
                          <input type='text' name='data[place2]' id='place2' value='' class="form-control" />
                          </td>
                          <th>{{$la.todaywater.arg1}}</th>
                          <td>
                          <input type='text' name='data[arg21]' id='arg21' value='' class="form-control" />
                          </td>
                          <th>{{$la.todaywater.arg2}}</th>
                          <td>
                          <input type='text' name='data[arg22]' id='arg22' value='' class="form-control" />
                          </td>
                       </tr>
                       <tr>
                          <th>{{$la.todaywater.gongshui}}3</th>
                          <td>
                          <input type='text' name='data[place3]' id='place3' value='' class="form-control" />
                          </td>
                          <th>{{$la.todaywater.arg1}}</th>
                          <td>
                          <input type='text' name='data[arg31]' id='arg31' value='' class="form-control" />
                          </td>
                          <th>{{$la.todaywater.arg2}}</th>
                          <td>
                          <input type='text' name='data[arg32]' id='arg32' value='' class="form-control" />
                          </td>
                       </tr>
						<tr>
						 <th></th>
                         <td colspan='2'><input type='submit' id='submit' class='btn btn-primary' value='{{$la.save}}' data-loading='{{$la.loading}}' />
                         &nbsp;&nbsp;&nbsp;<input type="button" class="btn btn-default goback" value="{{$la.goback}}" onclick="location.href='{:U('information/todaywater')}'">
                          </td>
                        </tr>
					</table>
				</form>
				</div>
			</div>
		</div>
	</div>
	{{include file="$WWW_PATH/footer.html"}}

    {{ex fn='exportJs'}}
</body>
</html>
