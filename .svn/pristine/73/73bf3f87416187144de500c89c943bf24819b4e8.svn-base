{{include file="$WWW_PATH/innerheader.html"}}
<body>
<script src="{:T('js/all.js')}"></script>
{{include file="$WWW_PATH/menu.html"}}
    <div class="fBn"><img src="{:T('img/fbn.jpg')}" /></div>
    {{include file="$WWW_PATH/rolling.html"}}
    <div class="wrap">
    	<div class="wL">
        	<div class="hd"><h3>栏目导航</h3></div>
            <div class="bd">
            	<ul>
                {{include file="$WWW_PATH/renliziyuan.html"}}
                </ul>
            </div>
        </div>
        <div class="wR">
        	<div class="bread">当前位置:<a href="{:U('home/default')}">首页</a>&gt;<span>提交简历</span></div>
            <div class="bd">
            <form action="{:U('hr/resume/add/insert.json')}" id="ajaxForm" >
            	<div class="resume">
                	<h2>提交简历</h2>
                    <div class="form rBasic">
                    	<h3>基本情况</h3>
                        <div class="mod">
                            <div class="l">姓名<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:150px;" name="data[name]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">性别<i>*</i></div>
                            <div class="r">
                                <select name='data[xingbie]' style="width:50px;height:28px;">
                                  {{html_options options=$xingbie_options}}
                                </select>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">生日<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:150px;" name="data[shengri]" placeholder="形如：1982-01-01"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">身份证号<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:150px;" name="data[shenfenzheng]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">民族<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:150px;" name="data[minzu]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">政治面貌<i>*</i></div>
                            <div class="r">
                                <select name='data[zhengzhi]' style="width:100px;height:28px;">
                                  {{html_options options=$zhengzhi_options}}
                                </select>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">籍贯<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:150px;" name="data[jiguan]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">婚姻状况<i>*</i></div>
                            <div class="r"><span class="formRad"><input type="radio" class="radio" name="data[hunyin]" id="unMar" checked value="未婚"/><label for="unMar">未婚</label><input type="radio" class="radio" name="data[hunyin]" id="mar" value="已婚"/><label for="mar">已婚</label></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">健康状况<i>&nbsp;</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:150px;" name="data[jiankang]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">身高<i>&nbsp;</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:120px;" name="data[shengao]"></span><span class="formTxt">cm</span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">体重<i>&nbsp;</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:120px;" name="data[tizhong]"></span><span class="formTxt">kg</span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">学历<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:150px;" name="data[xueli]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">专业<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:150px;" name="data[zhuanye]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod halfMod">
                            <div class="l">毕业院校<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:280px;" name="data[yuanxiao]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod halfMod">
                            <div class="l">户口地址<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:280px;" name="data[hukou]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod forthMod">
                            <div class="l">计算机水平<i>&nbsp;</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:90px;" name="data[jisuanji]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod forthMod">
                            <div class="l">驾驶水平<i>&nbsp;</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:90px;" name="data[jiashi]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod forthMod">
                            <div class="l">国语水平<i>&nbsp;</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:90px;" name="data[guoyu]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod forthMod">
                            <div class="l">外语水平<i>&nbsp;</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:90px;" name="data[waiyu]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod fullMod">
                            <div class="l">教育培训经历<i>&nbsp;</i></div>
                            <div class="r">
                                <span class="formTextarea">
                                    <textarea class="textarea" style="width:455px;" name="data[peixun]"></textarea>
                                </span>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod fullMod">
                            <div class="l">主要工作经历<i>&nbsp;</i></div>
                            <div class="r">
                                <span class="formTextarea">
                                    <textarea class="textarea" style="width:455px;" name="data[gongzuo]"></textarea>
                                </span>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod fullMod">
                            <div class="l">主要工作业绩<i>&nbsp;</i></div>
                            <div class="r">
                                <span class="formTextarea">
                                    <textarea class="textarea" style="width:455px;" name="data[yeji]"></textarea>
                                </span>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod fullMod">
                            <div class="l">个人特长爱好<i>&nbsp;</i></div>
                            <div class="r">
                                <span class="formTextarea">
                                    <textarea class="textarea" style="width:455px;" name="data[techang]"></textarea>
                                </span>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod fullMod">
                            <div class="l">家庭成员关系<i>&nbsp;</i></div>
                            <div class="r">
                                <span class="formTextarea">
                                    <textarea class="textarea" style="width:455px;" name="data[jiating]"></textarea>
                                </span>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="form job">
                    	<h3>应聘部门</h3>
                        <div class="mod">
                            <div class="l">部门名称<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" name="data[bumen]"></span><span class="formTxt">如果应聘多个，名称之间请用"|"隔开</span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">职位名称<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" name="data[zhiwei]"></span><span class="formTxt">如果应聘多个，名称之间请用"|"隔开</span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod">
                            <div class="l">个人要求<i>&nbsp;</i></div>
                            <div class="r">
                                <span class="formTextarea">
                                    <textarea class="textarea" style="width:455px;" name="data[yaoqiu]"></textarea>
                                </span>
                                <span class="formTxt">如：待遇、福利、发展等方面</span>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="form contactSelf">
                    	<h3>联系方式</h3>
                        <div class="mod halfMod">
                            <div class="l">联系电话<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" name="data[dianhua]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod halfMod">
                            <div class="l">电子邮箱<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" name="data[youxiang]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod fullMod">
                            <div class="l">QQ号码<i>&nbsp;</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" name="data[qq]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="mod fullMod">
                            <div class="l">通信地址<i>*</i></div>
                            <div class="r"><span class="formText"><input type="text" class="text" style="width:455px;" name="data[dizhi]"></span></div>
                            <div class="clear"></div>
                        </div>
                        <div class="clear"></div>
                    </div>
                    <div class="formBtn"><a href="javascript:;" class="btnsFb btn4" onclick="submitForm('ajaxForm')">提交</a><a href="javascript:;" onclick="reloadme();" class="btnsFb btn3">重置</a></div>
                </div>
                </form>
            </div>
        </div>
        <div class="clear"></div>
    </div>
    {{include file="$WWW_PATH/footer.html"}}
</div>
{{include file="$WWW_PATH/floatdiv.html"}}
