{{include file="$WWW_PATH/header.html"}}
<body>
{{ex fn='exportCss'}}
<header><h1>{{$la.person.common}}</h1><a href="{{$la.breadcrumb.homepage}}" class="back mw-icon">&#xf122;</a><a href="{{$la.breadcrumb.homepage}}" class="index mw-icon">&#xf123;</a></header>
<section class="content designlst">
    <div class="screen">
        <div class="tabT"><a href="javascript:void(0);" class="cu"><b>{{$la.person.team}}</b><i {{if ($team_choose)}}class="red"{{/if}}>({{if ($team_choose)}}{{$team_choose_val}}{{else}}{{$la.nochoose}}{{/if}})</i></a><a href="javascript:void(0);"><b>{{$la.person.level}}</b><i {{if ($level_choose)}}class="red"{{/if}}>({{if ($level_choose)}}{{$level_choose_val}}{{else}}{{$la.nochoose}}{{/if}})</i></a><a href="javascript:void(0);" style="display:none;"><b>{{$la.person.goodat}}</b><i {{if ($goodat_choose)}}class="red"{{/if}}>({{if ($goodat_choose)}}{{$goodat_choose_val}}{{else}}{{$la.nochoose}}{{/if}})</i></a></div>
        <form method="post" action="./design_person-list-list.html" name="myForm">
        <div class="tabC">
            <div class="tabCon" id="team">
                <ul>
                {{foreach from=$team_options key="k" item="v"}}
                    <li><a href="javascript:void(0);" {{if $team_choose == $k}}class="cu"{{/if}}><b>{{if $k == ""}}{{$la.all}}{{else}}{{$v}}{{/if}}</b><i class="mw-icon">{{if $team_choose == $k}}&#xf12d;{{else}}&#xf12c;{{/if}}</i></a>
                        <input type="radio" name="data[team]" value="{{$k}}" {{if $k == $team_choose}}checked ="checked"{{/if}}/>
                    </li>
                {{/foreach}}
                </ul>
                <div class="close"><a href="#"><i>×</i>关闭筛选</a></div>
            </div>
            <div class="tabCon" id="level">
                <ul>
                {{foreach from=$level_options key="k" item="v"}}
                    <li><a href="javascript:void(0);" {{if $level_choose == $k}}class="cu"{{/if}}><b>{{if $k == ""}}{{$la.all}}{{else}}{{$v}}{{/if}}</b><i class="mw-icon">{{if $level_choose == $k}}&#xf12d;{{else}}&#xf12c;{{/if}}</i></a>
                        <input type="radio" name="data[level]" value="{{$k}}" {{if $k == $level_choose}}checked ="checked"{{/if}}/>
                    </li>
                {{/foreach}}
                </ul>
                <div class="close"><a href="#"><i>×</i>关闭筛选</a></div>
            </div>
            <div class="tabCon" id="goodat">
                <ul>
                {{foreach from=$goodat_options key="k" item="v"}}
                    <li><a href="javascript:void(0);" {{if $goodat_choose == $k}}class="cu"{{/if}}><b>{{if $k == ""}}{{$la.all}}{{else}}{{$v}}{{/if}}</b><i class="mw-icon">{{if $goodat_choose == $k}}&#xf12d;{{else}}&#xf12c;{{/if}}</i></a>
                        <input type="radio" name="data[goodat]" value="{{$k}}" {{if $k == $goodat_choose}}checked ="checked"{{/if}}/>
                    </li>
                {{/foreach}}
                </ul>
                <div class="close"><a href="#"><i>×</i>关闭筛选</a></div>
            </div>
        </div>
        </form>
    </div>
    <div class="lst">
        <ul>
        {{foreach from=$list item="ls"}}
            <li><a href="./design_person-detail-default-{{$ls.id}}.html"><dl><dt><img src="{{$ls.url}}" /></dt><dd><div class="tit"><b>{{$ls.name}}</b><em>{{$ls.level}}</em></div><div class="txt"><p>{{$ls.ideal}}</p></div></dd></dl></a></li>
        {{/foreach}}
        </ul>
    </div>
    {{$pagelink}}
</section>
{{include file="$WWW_PATH/buttomstick.html"}}
{{include file="$WWW_PATH/footer.html"}}
{{ex fn='exportJs'}}
</body>
</html>
