{{include file="$WWW_PATH/header.html"}}
<body>
<header><h1>{{$la.bookonline.common}}</h1><a href="{{$la.breadcrumb.homepage}}" class="back mw-icon">&#xf122;</a><a href="{{$la.breadcrumb.homepage}}" class="index mw-icon">&#xf123;</a></header>
<form method="post" id="myForm" action="./interactive_bookonline-detail-insert.json">
<section class="content order">
    <div class="form">
        <div class="mod">
            <div class="formText"><input type="text" class="text" name="data[name]" id="ui-name" placeholder="{{$la.bookonline.name}}" /></div>
        </div>
        <div class="mod">
            <div class="formText"><input type="tel" class="text" name="data[tel]" id="ui-phone" placeholder="{{$la.bookonline.tel}}" /></div>
        </div>
        <div class="mod">
            <div class="formText"><input type="text" class="text" name="data[content]" id="ui-build" placeholder="{{$la.bookonline.content}}" /></div>
        </div>
        <div class="mod">
            <div class="formText"><textarea name="data[content2]" id="ui-info" class="textarea" placeholder="{{$la.bookonline.content2}}"></textarea></div>
        </div>
    </div>
    <div class="btn"><a href="javascript:void(0);" onclick="submitme('myForm');"class="btnbig btn2">{{$la.bookonline.submit}}</a></div>
</section>
</form>
{{include file="$WWW_PATH/buttomstick.html"}}
{{include file="$WWW_PATH/footer.html"}}
{{ex fn='exportJs'}}
</body>
</html>
