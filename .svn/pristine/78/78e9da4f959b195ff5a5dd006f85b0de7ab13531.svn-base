{{include file="$WWW_PATH/header.html"}}
<body>
    {{include file="$WWW_PATH/top_menu.html"}}

	<div class="clearfix">
    {{include file="$WWW_PATH/left_menu.html"}}
		<div class='col-md-10'>
			<div class='panel'>
				<div class='panel-heading'>
					<strong><i class='icon-list-ul'></i>&nbsp;{{$la.cellphone.common}}</strong>
				</div>
				{{if !empty($list)}}
				<!-- <form id='sortForm'
					action='/chanzhieps/www/admin.php?m=slide&f=sort' method='post'>-->
				  <table class='table table-hover table-striped tablesorter'>
				    <thead>
				      <tr>
				        <th class='text-center w-100px'>{{$la.cellphone.name}}
				</div></th>
				        <th class='text-center w-100px'>{{$la.cellphone.tel}}
                </div></th>
				        <th class='text-center w-60px'><div class='{{if $orderby=="createtime" and $sort=="desc"}}headerSortDown{{elseif $orderby=="createtime" and $sort=="asc"}}headerSortUp{{else}}header{{/if}}'><a href='manage_cellphone-default-sort-currpage-{{$currpage}}-orderby-createtime-sort-{{if $orderby == "createtime"}}{{$activesorting}}{{else}}{{$sorting}}{{/if}}.html' >{{$la.cellphone.createtime}}</a>
                </div></th>
				        <th class='text-center w-60px'><div class='{{if $orderby=="public" and $sort=="desc"}}headerSortDown{{elseif $orderby=="public" and $sort=="asc"}}headerSortUp{{else}}header{{/if}}'><a href='manage_cellphone-default-sort-currpage-{{$currpage}}-orderby-public-sort-{{if $orderby == "public"}}{{$activesorting}}{{else}}{{$sorting}}{{/if}}.html' >{{$la.cellphone.publicstatus}}</a>
                </div></th>
				        <th class='text-center w-120px'>{{$la.actions}}</th>
				      </tr>
				    </thead>
				    <tbody>
				    {{foreach from=$list item="kv"}}
                        <tr>
                        <td class='text-center'>{{$kv.name}}</td>
                        <td class='text-center'>{{$kv.tel}}</td>
                        <td class='text-center'>{{$kv.createtime}}</td>
                        <td class='text-center {{if $kv.public == "1"}}is-visible{{/if}}'>{{if $kv.public == "1"}}{{$la.cellphone.published}}{{else}}{{$la.cellphone.unpublished}}{{/if}}</td>
                        <td class='text-center'>
                        {{if $kv.public == "0"}}
                          <a href='manage_cellphone-default-publish-{{$kv.id}}.json' class="publisher">{{$la.cellphone.public}}</a>
                        {{/if}}
                          <a href='manage_cellphone-edit-default-id-{{$kv.id}}.html' >{{$la.cellphone.editor}}</a>
			                <!--
			                <a href='system_file-default-default-objecttype-cellphone-objectid-{{$kv.id}}.modal' data-toggle='modal'>{{$la.cellphone.files}}</a>
			                 -->
			                <a href='manage_cellphone-default-delete-id-{{$kv.id}}.json' class="deleter">{{$la.delete}}</a>
                        </td>
                        </tr>
				    {{/foreach}}
				          </tbody>
				    <tfoot><tr><td colspan='8'>
				    <div style='float:right; clear:none;' class='page form-inline'>
				    {{$pagelink}}
				        </div></td></tr></tfoot>
				  </table>
				  {{/if}}
			</div>
		</div>
	</div>
	{{include file="$WWW_PATH/footer.html"}}

    {{ex fn='exportJs'}}
</body>
</html>
